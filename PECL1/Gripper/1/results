
(:action pick-up
    :parameters (ball4 rooma right)
    :precondition
    (and
      (ball ball4)
      (room rooma)
      (gripper right)
      (at-ball ball4 rooma)
      (at-robby rooma)
      (free right)
    )
    :effect
    (and
      (carry right ball4)
      (not
        (at-ball ball4 rooma)
      )
      (not
        (free right)
      )
    )
)

  (:action move
    :parameters (rooma roomb)
    :precondition
      (and
        (room rooma)
        (room roomb)
        (at-robby rooma)
      )
    :effect
      (and
        (at-robby roomb)
        (not
          (at-robby rooma)
        )
      )
  )

 (:action drop
    :parameters (ball4 roomb right)
    :precondition
      (and
        (ball ball4)
        (room roomb)
        (gripper right)
        (carry right ball4)
        (at-robby roomb)
      )
    :effect
      (and
        (at-ball ball4 roomb)
        (free right)
        (not
          (carry right ball4)
        )
      )
  )

 (:action move
    :parameters (roomb rooma)
    :precondition
      (and
        (room roomb)
        (room rooma)
        (at-robby roomb)
      )
    :effect
      (and
        (at-robby rooma)
        (not
          (at-robby roomb)
        )
      )
  )

